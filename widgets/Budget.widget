            },
            "percent": {
              "type": "number"
            }
          },
          "required": [
            "id",
            "icon",
            "label",
            "amount",
            "percent"
          ],
          "additionalProperties": false
        }
      },
      "chart": {
        "type": "object",
        "properties": {
          "data": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "value": {
                  "type": "number"
                }
              },
              "required": [
                "name",
                "value"
              ],
              "additionalProperties": false
            }
          },
          "barColor": {
            "type": "string"
          }
        },
        "required": [
          "data",
          "barColor"
        ],
        "additionalProperties": false
      },
      "theme": {
        "type": "object",
        "properties": {
          "lightTeal": {
            "type": "string"
          },
          "darkTeal": {
            "type": "string"
          },
          "gray": {
            "type": "string"
          },
          "white": {
            "type": "string"
          }
        },
        "required": [
          "lightTeal",
          "darkTeal",
          "gray",
          "white"
        ],
        "additionalProperties": false
      }
    },
    "required": [
      "title",
      "currency",
      "totalFormatted",
      "perDayFormatted",
      "items",
      "chart",
      "theme"
    ],
    "additionalProperties": false
  },
