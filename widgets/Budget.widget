  "template": "{\"type\":\"Card\",\"size\":\"md\",\"padding\":0,\"children\":[{\"type\":\"Col\",\"gap\":0,\"children\":[{\"type\":\"Box\",\"background\":{{ (theme.darkTeal) | tojson }},\"padding\":4,\"children\":[{\"type\":\"Col\",\"gap\":1,\"children\":[{\"type\":\"Caption\",\"value\":{{ (currency) | tojson }},\"color\":{{ (theme.white) | tojson }}},{\"type\":\"Title\",\"value\":{{ (title) | tojson }},\"size\":\"md\",\"color\":{{ (theme.white) | tojson }}}]}]},{\"type\":\"Col\",\"gap\":3,\"padding\":4,\"children\":[{\"type\":\"Row\",\"gap\":3,\"children\":[{\"type\":\"Box\",\"flex\":1,\"background\":{{ (theme.white) | tojson }},\"border\":{\"size\":1,\"color\":\"subtle\"},\"radius\":\"lg\",\"padding\":3,\"children\":[{\"type\":\"Text\",\"value\":\"Total\",\"size\":\"sm\",\"color\":{{ (theme.gray) | tojson }}},{\"type\":\"Title\",\"value\":{{ (totalFormatted) | tojson }},\"size\":\"md\"}]},{\"type\":\"Box\",\"flex\":1,\"background\":{{ (theme.white) | tojson }},\"border\":{\"size\":1,\"color\":\"subtle\"},\"radius\":\"lg\",\"padding\":3,\"children\":[{\"type\":\"Text\",\"value\":\"Per day\",\"size\":\"sm\",\"color\":{{ (theme.gray) | tojson }}},{\"type\":\"Title\",\"value\":{{ (perDayFormatted) | tojson }},\"size\":\"md\"}]}]},{\"type\":\"Box\",\"background\":{{ (theme.white) | tojson }},\"border\":{\"size\":1,\"color\":\"subtle\"},\"radius\":\"lg\",\"padding\":3,\"children\":[{\"type\":\"Chart\",\"data\":{{ (chart.data) | tojson }},\"series\":[{\"type\":\"Text\",\"value\":{\"type\":\"bar\",\"dataKey\":\"value\",\"color\":{{ (chart.barColor) | tojson }}}}],\"xAxis\":{\"dataKey\":\"name\"},\"showYAxis\":false,\"showLegend\":false,\"height\":140}]},{\"type\":\"Col\",\"gap\":2,\"children\":[{%- set _c -%}{%-for item in items -%},{\"type\":\"Row\",\"key\":{{ (item.id) | tojson }},\"gap\":3,\"align\":\"center\",\"children\":[{\"type\":\"Box\",\"background\":{{ (theme.lightTeal) | tojson }},\"radius\":\"sm\",\"padding\":2,\"children\":[{\"type\":\"Icon\",\"name\":{{ (item.icon) | tojson }},\"color\":{{ (theme.darkTeal) | tojson }}}]},{\"type\":\"Col\",\"children\":[{\"type\":\"Text\",\"value\":{{ (item.label) | tojson }},\"size\":\"sm\",\"weight\":\"semibold\"},{\"type\":\"Caption\",\"value\":{{ ((item.percent ~ \"%\")) | tojson }},\"color\":\"secondary\"}]},{\"type\":\"Spacer\"},{\"type\":\"Text\",\"value\":{{ (item.amount) | tojson }},\"size\":\"sm\",\"weight\":\"semibold\"}]}{%-endfor-%}{%- endset -%}{{- (_c[1:] if _c and _c[0] == ',' else _c) -}}]},{\"type\":\"Row\",\"gap\":2,\"children\":[{\"type\":\"Button\",\"label\":\"Save\",\"style\":\"primary\",\"onClickAction\":{\"type\":\"budget.save\",\"payload\":{\"title\":{{ (title) | tojson }}}}},{\"type\":\"Button\",\"label\":\"Adjust\",\"variant\":\"outline\",\"onClickAction\":{\"type\":\"budget.adjust\",\"payload\":{\"title\":{{ (title) | tojson }}}}},{\"type\":\"Button\",\"label\":\"Share\",\"variant\":\"outline\",\"iconStart\":\"external-link\",\"onClickAction\":{\"type\":\"budget.share\",\"payload\":{\"title\":{{ (title) | tojson }}}}}]}]}]}]}",
  "jsonSchema": {
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
      "title": {
        "type": "string"
      },
      "currency": {
        "type": "string"
      },
      "totalFormatted": {
        "type": "string"
      },
      "perDayFormatted": {
        "type": "string"
      },
      "items": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string"
            },
            "icon": {
              "type": "string",
