{
  "version": "1.0",
  "name": "Dua Widget",
  "template": "{\"type\":\"Card\",\"size\":\"md\",\"padding\":0,\"children\":[{\"type\":\"Box\",\"background\":{{ (palette.headerBg) | tojson }},\"padding\":{\"x\":4,\"y\":4},\"children\":[{\"type\":\"Col\",\"gap\":1,\"children\":[{\"type\":\"Caption\",\"value\":{{ (data.occasion) | tojson }},\"color\":\"white\"},{\"type\":\"Title\",\"value\":{{ (data.title) | tojson }},\"size\":\"md\",\"color\":\"white\"}]}]},{\"type\":\"Col\",\"padding\":{\"x\":4,\"y\":3},\"gap\":3,\"children\":[{\"type\":\"Title\",\"value\":{{ (data.arabic) | tojson }},\"size\":\"lg\",\"color\":{{ (palette.darkTeal) | tojson }}},{\"type\":\"Text\",\"value\":{{ (data.transliteration) | tojson }},\"size\":\"sm\",\"italic\":true,\"color\":\"secondary\"},{\"type\":\"Text\",\"value\":{{ (data.translation) | tojson }},\"size\":\"sm\",\"color\":\"emphasis\"},{\"type\":\"Row\",\"gap\":2,\"children\":[{\"type\":\"Box\",\"background\":{{ (palette.lightTeal) | tojson }},\"radius\":\"sm\",\"padding\":1,\"children\":[{\"type\":\"Icon\",\"name\":\"book-open\",\"size\":\"sm\",\"color\":{{ (palette.darkTeal) | tojson }}}]},{\"type\":\"Caption\",\"value\":{{ (data.reference) | tojson }},\"color\":\"secondary\"}]}]},{\"type\":\"Divider\",\"flush\":true},{\"type\":\"Row\",\"padding\":{\"x\":4,\"y\":3},\"gap\":3,\"children\":[{\"type\":\"Button\",\"label\":\"Play audio\",\"style\":\"primary\",\"iconStart\":\"play\",\"onClickAction\":{\"type\":\"dua.play_audio\",\"payload\":{\"src\":{{ (data.audioUrl) | tojson }},\"title\":{{ (data.title) | tojson }}}}},{\"type\":\"Spacer\"},{\"type\":\"Button\",\"iconStart\":\"star\",\"variant\":\"outline\",\"uniform\":true,\"onClickAction\":{\"type\":\"dua.bookmark\",\"payload\":{\"title\":{{ (data.title) | tojson }}}}},{\"type\":\"Button\",\"iconStart\":\"external-link\",\"variant\":\"outline\",\"uniform\":true,\"onClickAction\":{\"type\":\"dua.share\",\"payload\":{\"title\":{{ (data.title) | tojson }},\"url\":{{ (data.audioUrl) | tojson }}}}},{\"type\":\"Button\",\"iconStart\":\"square-text\",\"variant\":\"outline\",\"uniform\":true,\"onClickAction\":{\"type\":\"dua.copy\",\"payload\":{\"text\":{{ ((data.title ~ \"\\n\" ~ data.occasion ~ \"\\n\" ~ data.arabic ~ \"\\n\" ~ data.transliteration ~ \"\\n\" ~ data.translation ~ \"\\n\" ~ data.reference)) | tojson }}}}}]}]}",
  "jsonSchema": {
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
      "type": {
        "type": "string",
        "const": "dua"
      },
      "data": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string"
          },
          "occasion": {
            "type": "string"
          },
          "arabic": {
            "type": "string"
          },
          "transliteration": {
            "type": "string"
          },
          "translation": {
            "type": "string"
          },
          "audioUrl": {
            "type": "string"
          },
          "reference": {
            "type": "string"
          }
        },
        "required": [
          "title",
          "occasion",
          "arabic",
          "transliteration",
          "translation",
          "audioUrl",
          "reference"
        ],
        "additionalProperties": false
      },
      "actions": {
        "type": "array",
        "items": {
          "type": "string",
          "enum": [
            "play_audio",
            "bookmark",
            "share",
            "copy"
          ]
        }
      },
      "palette": {
        "type": "object",
        "properties": {
          "headerBg": {
            "type": "string"
          },
          "darkTeal": {
            "type": "string"
          },
          "lightTeal": {
            "type": "string"
          },
          "gray": {
            "type": "string"
          }
        },
        "required": [
          "headerBg",
          "darkTeal",
          "lightTeal",
          "gray"
        ],
        "additionalProperties": false
      }
    },
    "required": [
      "type",
      "data",
      "actions",
      "palette"
    ],
    "additionalProperties": false
  },
  "outputJsonPreview": {
    "type": "Card",
    "size": "md",
    "padding": 0,
    "children": [
      {
        "type": "Box",
        "background": "linear-gradient(135deg, #ffffff 0%, #ccfbf1 30%, #2dd4bf 65%, #0f766e 100%)",
        "padding": {
          "x": 4,
          "y": 4
        },
        "children": [
          {
            "type": "Col",
            "gap": 1,
            "children": [
              {
                "type": "Caption",
                "value": "While performing Tawaf",
                "color": "white"
              },
              {
                "type": "Title",
                "value": "Dua for Tawaf",
                "size": "md",
                "color": "white"
              }
            ]
          }
        ]
      },
      {
        "type": "Col",
        "padding": {
          "x": 4,
          "y": 3
        },
        "gap": 3,
        "children": [
          {
            "type": "Title",
            "value": "رَبَّنَا آتِنَا فِي الدُّنْيَا حَسَنَةً...",
            "size": "lg",
            "color": "#0f766e"
          },
          {
            "type": "Text",
            "value": "Rabbana atina fid-dunya hasanatan...",
            "size": "sm",
            "italic": true,
            "color": "secondary"
          },
          {
            "type": "Text",
            "value": "Our Lord, give us good in this world...",
            "size": "sm",
            "color": "emphasis"
          },
          {
            "type": "Row",
            "gap": 2,
            "children": [
              {
                "type": "Box",
                "background": "#ccfbf1",
                "radius": "sm",
                "padding": 1,
                "children": [
                  {
                    "type": "Icon",
                    "name": "book-open",
                    "size": "sm",
                    "color": "#0f766e"
                  }
                ]
              },
              {
                "type": "Caption",
                "value": "Quran 2:201",
                "color": "secondary"
              }
            ]
          }
        ]
      },
      {
        "type": "Divider",
        "flush": true
      },
      {
        "type": "Row",
        "padding": {
          "x": 4,
          "y": 3
        },
        "gap": 3,
        "children": [
          {
            "type": "Button",
            "label": "Play audio",
            "style": "primary",
            "iconStart": "play",
            "onClickAction": {
              "type": "dua.play_audio",
              "payload": {
                "src": "/audio/dua-tawaf.mp3",
                "title": "Dua for Tawaf"
              }
            }
          },
          {
            "type": "Spacer"
          },
          {
            "type": "Button",
            "iconStart": "star",
            "variant": "outline",
            "uniform": true,
            "onClickAction": {
              "type": "dua.bookmark",
              "payload": {
                "title": "Dua for Tawaf"
              }
            }
          },
          {
            "type": "Button",
            "iconStart": "external-link",
            "variant": "outline",
            "uniform": true,
            "onClickAction": {
              "type": "dua.share",
              "payload": {
                "title": "Dua for Tawaf",
                "url": "/audio/dua-tawaf.mp3"
              }
            }
          },
          {
            "type": "Button",
            "iconStart": "square-text",
            "variant": "outline",
            "uniform": true,
            "onClickAction": {
              "type": "dua.copy",
              "payload": {
                "text": "Dua for Tawaf\nWhile performing Tawaf\nرَبَّنَا آتِنَا فِي الدُّنْيَا حَسَنَةً...\nRabbana atina fid-dunya hasanatan...\nOur Lord, give us good in this world...\nQuran 2:201"
              }
            }
          }
        ]
      }
    ]
  },
