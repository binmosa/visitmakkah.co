{
  "version": "1.0",
  "name": "Places widget",
  "template": "{\"type\":\"Card\",\"size\":\"md\",\"padding\":0,\"theme\":\"light\",\"children\":[{\"type\":\"Col\",\"gap\":0,\"children\":[{\"type\":\"Box\",\"background\":{\"light\":\"teal-600\",\"dark\":\"teal-700\"},\"padding\":{\"x\":4,\"y\":3},\"children\":[{\"type\":\"Row\",\"align\":\"center\",\"gap\":3,\"children\":[{\"type\":\"Col\",\"gap\":1,\"children\":[{\"type\":\"Caption\",\"value\":{{ (data.category) | tojson }},\"color\":{\"light\":\"white\",\"dark\":\"white\"}},{\"type\":\"Title\",\"value\":{{ (data.title) | tojson }},\"size\":\"md\",\"color\":{\"light\":\"white\",\"dark\":\"white\"}}]},{\"type\":\"Spacer\"},{\"type\":\"Row\",\"gap\":2,\"children\":[{\"type\":\"Button\",\"label\":\"Map\",\"iconStart\":\"maps\",\"variant\":\"soft\",\"color\":\"info\",\"onClickAction\":{\"type\":\"places.show_map\",\"payload\":{\"items\":{{ (data[\"items\"]) | tojson }}}}},{\"type\":\"Button\",\"label\":\"Filters\",\"iconStart\":\"settings-slider\",\"variant\":\"outline\",\"color\":\"secondary\",\"onClickAction\":{\"type\":\"places.filter\",\"payload\":{\"filters\":{{ (data.filters) | tojson }},\"category\":{{ (data.category) | tojson }}}}}]}]}]},{\"type\":\"Col\",\"padding\":{\"x\":4,\"y\":2},\"gap\":2,\"background\":{\"light\":\"teal-100\",\"dark\":\"teal-100\"},\"children\":[{\"type\":\"Row\",\"wrap\":\"wrap\",\"gap\":2,\"children\":[{%- set _c -%}{%-for f in data.filters -%}{%-set i = loop.index0 -%},{\"type\":\"Button\",\"key\":{{ ((f ~ \"-\" ~ i)) | tojson }},\"label\":{{ (f) | tojson }},\"size\":\"xs\",\"variant\":\"soft\",\"color\":\"info\",\"onClickAction\":{\"type\":\"places.filter\",\"payload\":{\"key\":{{ (f) | tojson }},\"category\":{{ (data.category) | tojson }}}}}{%-endfor-%}{%- endset -%}{{- (_c[1:] if _c and _c[0] == ',' else _c) -}}]}]},{\"type\":\"Divider\"},{\"type\":\"ListView\",\"children\":[{%- set _c -%}{%-for item in data[\"items\"] -%},{\"type\":\"ListViewItem\",\"key\":{{ (item.id) | tojson }},\"align\":\"stretch\",\"gap\":3,\"onClickAction\":{\"type\":\"places.select\",\"payload\":{\"id\":{{ (item.id) | tojson }}}},\"children\":[{\"type\":\"Image\",\"src\":{{ (item.image) | tojson }},\"alt\":{{ (item.name) | tojson }},\"size\":64,\"radius\":\"lg\",\"fit\":\"cover\"},{\"type\":\"Col\",\"flex\":1,\"gap\":1,\"children\":[{\"type\":\"Row\",\"children\":[{\"type\":\"Text\",\"value\":{{ (item.name) | tojson }},\"size\":\"sm\",\"weight\":\"semibold\"},{\"type\":\"Spacer\"},{\"type\":\"Badge\",\"label\":{{ ((item.price.currency ~ \" \" ~ item.price.amount ~ \"/\" ~ item.price.per)) | tojson }},\"color\":\"info\",\"size\":\"sm\"}]},{\"type\":\"Row\",\"gap\":3,\"children\":[{\"type\":\"Row\",\"gap\":1,\"children\":[{\"type\":\"Icon\",\"name\":\"star-filled\",\"color\":\"yellow\",\"size\":\"sm\"},{\"type\":\"Text\",\"value\":{{ ((item.rating ~ \" (\" ~ item.reviews ~ \")\")) | tojson }},\"size\":\"sm\",\"color\":\"secondary\"}]},{\"type\":\"Spacer\"},{\"type\":\"Caption\",\"value\":{{ (item.distance) | tojson }},\"color\":\"secondary\"}]},{\"type\":\"Row\",\"wrap\":\"wrap\",\"gap\":1,\"children\":[{%- set _c -%}{%-for a in item.amenities -%}{%-set idx = loop.index0 -%},{\"type\":\"Badge\",\"key\":{{ ((item.id ~ \"-amenity-\" ~ idx)) | tojson }},\"label\":{{ (a) | tojson }},\"variant\":\"outline\",\"size\":\"sm\"}{%-endfor-%}{%- endset -%}{{- (_c[1:] if _c and _c[0] == ',' else _c) -}}]}]},{\"type\":\"Col\",\"align\":\"end\",\"gap\":2,\"children\":[{\"type\":\"Button\",\"label\":\"Book\",\"size\":\"sm\",\"color\":\"success\",\"onClickAction\":{\"type\":\"places.book\",\"payload\":{\"id\":{{ (item.id) | tojson }}}}},{\"type\":\"Button\",\"iconStart\":\"star\",\"size\":\"sm\",\"variant\":\"outline\",\"onClickAction\":{\"type\":\"places.save\",\"payload\":{\"id\":{{ (item.id) | tojson }}}}}]}]}{%-endfor-%}{%- endset -%}{{- (_c[1:] if _c and _c[0] == ',' else _c) -}}]}]}]}",
  "jsonSchema": {
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
      "type": {
        "type": "string",
        "const": "places"
